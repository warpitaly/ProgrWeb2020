<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>07Dati</display-name>
    <listener>
        <listener-class>it.units.progrweb2020.dati.utils.OfyStarter</listener-class>
    </listener>
    <servlet>
        <servlet-name>SalvaManuale</servlet-name>
        <servlet-class>it.units.progrweb2020.dati.servlets.SalvaManuale</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SalvaOfy</servlet-name>
        <servlet-class>it.units.progrweb2020.dati.servlets.SalvaOfy</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SalvaManuale</servlet-name>
        <url-pattern>/SalvaManuale</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SalvaOfy</servlet-name>
        <url-pattern>/SalvaOfy</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
      30
    </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
